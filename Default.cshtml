<!DOCTYPE html>
<html>
    <head>
        <title>Lil Syncr</title>
        <link href="~/Styles/Syncr.css" rel="stylesheet" type="text/css" />
        <script src="http://maps.google.com/maps/api/js?sensor=false"></script> 
        <script src="~/Scripts/jquery-1.8.1.min.js"></script>
        <script src="~/Scripts/jquery.gomap-1.3.2.min.js"></script> 
        <script src="~/Scripts/Script.js"></script>
    </head>

    <body>
        <div id="wrapper">
            <div id="backButton"></div>
            <div id="header"><h1 class="pageTitle">Lil Syncr</h1></div>
            <div id="content">
                <div class="col" id="col-Map">
                    <h2>Map</h2>
                    <div id="boxMap"></div>
                    <a id="biggerMap" href="/#">view larger map</a>
                </div>
                <div class="col" id="col-People">
                    <h2 class="colName">People</h2>
                    <input type="text" name="filter" value="" id="filter" class="serif" />
                    <div id="peopleBox"></div>
                </div>
                <div class="col" id="col-Settings">
                    <h2 class="colName">Sync</h2>
                    <input type="submit" value="upload" id="uploadButton" class="metroButton" />
                    <table id="statusTable">
                        <tr class="uploading">
                            <td>Uploading</td>
                            <td class="alignRight">...</td>
                        </tr>
                        <tr class="processing">
                            <td>Processing</td>
                            <td class="alignRight">...</td>
                        </tr>
                        <tr class="maps">
                            <td>Map Sync</li>
                            <td class="alignRight">...</li>
                        </tr>
                        <tr class="facebook">
                            <td>Facebook Sync</td>
                            <td class="alignRight">...</td>
                        </tr>
                        <tr class="gmail">
                            <td>Gmail Sync</td>
                            <td class="alignRight">...</td>
                        </tr>
                        <tr class="mailchimp">
                            <td>MailChimp Sync</td>
                            <td class="alignRight">...</td>
                        </tr>    
                    </table>
                </div>
                <div class="col" id="col-Details">
                    <h2 class="colName"></h2>
                    <div id="ProfilePic" class="detailBox"></div>
                    <div id="Contact" class="detailBox"></div>
                    <div id="Notifications" class="detailBox"></div>
                    <div id="Kids">
                        <h3>Kids</h3>
                        <div id="Kid1"class="detailBox"></div>
                        <div id="Kid2"class="detailBox"></div>
                        <div id="Kid3"class="detailBox"></div>
                    </div>
                </div>    
            </div>
        </div>
  </body>
</html>